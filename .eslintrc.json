{
    "env": {
      "browser": true,
      "commonjs": true,
      "es6": true
    },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "jsx": true,
        "arrowFunctions": true
      },
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "plugins": ["react"],
    "rules": {
      "indent": ["error", 2, { "SwitchCase": 1 }],
      "linebreak-style": ["error", "unix"],
      // "quotes": ["error", "single"],
      "semi": ["warn", "always"],
      "react/prop-types": 0,
      "react/display-name": 0
    }
  }